                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
                        <subviews>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" editable="NO" usesAttributedText="YES" selectable="NO" id="UMv-P0-yVN" customClass="AppTextView" customModule="NetanyaJunior" customModuleProvider="target">
                                <rect key="frame" x="0.0" y="65" width="320" height="458"/>
