                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" editable="NO" textAlignment="right" selectable="NO" id="6VL-l3-ThR" customClass="AppTextView" customModule="NetanyaJunior" customModuleProvider="target">
