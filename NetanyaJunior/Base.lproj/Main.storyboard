                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" editable="NO" textAlignment="right" selectable="NO" id="q6c-O2-Evc" customClass="AppTextView" customModule="NetanyaJunior" customModuleProvider="target">
